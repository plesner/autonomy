<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:c="urn:import:com.google.gwt.canvas.client">
<ui:style>
.outer {
  position: relative;
  width: 100%;
  height: 100%;
}
.inner {
  display: block;
}
</ui:style>
  <g:FlowPanel ui:field="container" stylePrimaryName="{style.outer}">
    <c:Canvas ui:field="canvas" styleName="{style.inner}"></c:Canvas>
  </g:FlowPanel>
</ui:UiBinder>
