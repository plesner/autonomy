<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:t="urn:import:org.au.tonomy.client.widget">
  <ui:style src="MainWidget.css"></ui:style>
  <g:HorizontalPanel>
    <t:CodeEditorWidget
        ui:field="editor"
        styleName="{style.editor}">
    </t:CodeEditorWidget>
    <t:WorldWidget
        ui:field="world"
        styleName="{style.world}">
    </t:WorldWidget>
  </g:HorizontalPanel>
</ui:UiBinder> 
