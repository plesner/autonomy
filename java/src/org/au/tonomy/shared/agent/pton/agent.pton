- settings:
    package: org.au.tonomy.shared.agent.pton

- type: DocumentData
  fields:
    text: string
    fingerprint: FingerprintData

- type: FingerprintData
  implements:
    - org.au.tonomy.shared.ot.IFingerprint
  fields:
    variant: string
    payload: string

- type: FileData
  fields:
    id: i32
    name: string
    path: string

- type: SessionData
  fields:
    id: string

- type: OperationData
  fields:
    type: i32
    count: i32
    text: string
