- settings:
    package: org.au.tonomy.shared.plankton

- service: Agent

- command: ls
  target: Agent
  payload:
    session: string
    file: i32

- command: fileroots
  target: Agent
  payload:
    session: string

- command: read
  target: Agent
  payload:
    session: string
    file: i32

- type: Document
  fields:
    text: string
    fingerprint: Fingerprint

- type: Fingerprint
  implements:
    - org.au.tonomy.shared.ot.IFingerprint
  fields:
    variant: string
    payload: string
