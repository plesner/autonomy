- settings:
    package: test.org.au.tonomy.shared.plankton

- type: Point
  fields:
    x: i32
    y: i32

- type: Rect
  fields:
    top_left: Point
    bottom_right: Point

- service: MathService

- command: mult
  target: MathService
  in:
    a: i32
    b: i32
  out: i32

- command: add
  target: MathService
  in:
    a: i32
    b: i32
  out: i32

- command: neg
  target: MathService
  in:
    value: i32
  out: i32
